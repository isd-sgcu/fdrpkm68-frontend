    "format:check": "prettier --check .",
    "clean:imports": "eslint . --ext .js,.ts,.tsx,.astro --fix --quiet",
    "type-check": "astro check",
    "quality": "pnpm type-check && pnpm lint",
    "prepare": "husky"
