    "@fontsource/zen-dots": "^5.2.6",
    "@lucide/astro": "^0.525.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@yudiel/react-qr-scanner": "^2.3.1",
    "astro": "^5.10.1",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.0.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
